{
	"name": "autoreserve",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "./popup/popup.html"
	},
	"permissions":["tabs"],
	"content_scripts":[
        {
            "matches": ["http://*/*"],
            "js": ["jquery.js", "main.js"]
        }
    ],
	"web_accessible_resources": ["icon.png"]
}